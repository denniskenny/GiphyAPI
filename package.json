{
    "name": "javascript-development-environment",
    "version": "1.0.0",
    "description": "Giphy Search Demo",
    "scripts": {
        "prestart": "babel-node buildScripts/startMessage.js",
        "start": "npm-run-all --parallel open:src lint:watch test:watch",
        "open:src": "babel-node buildScripts/srcServer.js",
        "lint": "esw webpack.config.* src buildScripts --color",
        "lint:watch": "npm run lint -- --watch",
        "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
        "test:watch": "npm run test -- --watch",
        "clean-dist": "rimraf ./dist && mkdir dist",
        "prebuild": "npm-run-all clean-dist test lint",
        "build": "babel-node buildScripts/build.js",
        "postbuild": "babel-node buildScripts/distServer.js",
        "deploy": "surge ./dist",
        "clean-debug": "rimraf ./debug",
        "make-debugfiles": "babel *.js src/*.js src/**/*.js api/*.* buildScripts/*.* --out-dir debug --source-maps",
        "debugBuild": "npm-run-all clean-debug make-debugfiles"
    },
    "author": "Sean Fitzgerald",
    "license": "MIT",
    "dependencies": {
        "angular": "^1.6.2",
        "angular-resource": "^1.6.2",
        "angular-route": "^1.6.2",
        "body-parser": "^1.17.1",
        "bootstrap": "^3.3.7",
        "jquery": "^3.1.1",
        "lodash": "^4.17.4",
        "whatwg-fetch": "1.0.0"
    },
    "devDependencies": {
        "babel-cli": "6.16.0",
        "babel-core": "6.17.0",
        "babel-loader": "6.2.5",
        "babel-preset-latest": "6.16.0",
        "babel-register": "6.16.3",
        "chai": "3.5.0",
        "chalk": "1.1.3",
        "cheerio": "0.22.0",
        "compression": "1.6.2",
        "copy-webpack-plugin": "^4.0.1",
        "cross-env": "3.1.3",
        "css-loader": "0.25.0",
        "eslint": "3.8.1",
        "eslint-plugin-import": "2.0.1",
        "eslint-watch": "2.1.14",
        "express": "4.14.0",
        "extract-text-webpack-plugin": "1.0.1",
        "file-loader": "^0.10.1",
        "html-webpack-plugin": "2.22.0",
        "jsdom": "9.8.0",
        "json-schema-faker": "0.3.6",
        "json-server": "0.8.22",
        "localtunnel": "1.8.1",
        "mocha": "3.1.2",
        "nock": "8.1.0",
        "npm-run-all": "3.1.1",
        "numeral": "1.5.3",
        "open": "0.0.5",
        "rimraf": "2.5.4",
        "style-loader": "0.13.1",
        "surge": "0.18.0",
        "webpack": "1.13.2",
        "webpack-dev-middleware": "1.8.4",
        "webpack-hot-middleware": "2.13.0",
        "webpack-md5-hash": "0.0.5"
    }
}
